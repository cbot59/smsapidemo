<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<package name="Struts2SpringDemo" extends="struts-default">
		<action name="login" class="com.aldi.smsapi.action.LoginAction">
			<result name="input">/LoginForm.jsp</result>
			<result name="success" type="velocity">/LoginSuccess.vm</result>
			<result name="error">/LoginError.jsp</result>
		</action>
	</package>
</struts>